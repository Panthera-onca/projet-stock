{% extends 'base.html.twig' %}

{% block main %}

                <h1>Enregistrer!</h1>
                {{ form_start(form) }}
                {{ form_row(form.nom) }}
                {{ form_row(form.prenom) }}
                {{ form_row(form.email) }}
                {{ form_row(form.username) }}
                {{ form_row(form.password) }}
                <button type="submit" class="btn btn-success">S'inscrire <span class="fa fa-lock"></span></button>
                &nbsp;
                <a href="{{ path('register') }}">Enregistrer</a>
                {{ form_end(form) }}

{% endblock %}

{% block title %}Enregistrer | {{ parent() }}{% endblock %}